#include<iostream>
#include<bits/stdc++.h>

using namespace std;

//check the given string is palindrome or not
bool CheckPalindrome(int i,int n,string s)
{
    if(i>=n/2)
    {
        return true;
    }
    if(s[i]!=s[n-i-1])
    {
        return false;
    }
    return CheckPalindrome(i+1,n,s);
}
int main()
{
    string s="MADAM";
    
    cout<<CheckPalindrome(0,n,s);
}
